    "build": "tsc -b",
    "start": "tsx src/bin.ts"
  },
  "devDependencies": {
    "@types/node": "^20.1.7",
    "prettier": "^2.8.8",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "fets": "^0.2.8",
    "yaml": "^2.3.1",
    "zod": "^3.21.4"
  }
}
