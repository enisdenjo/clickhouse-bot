    "start": "node src/bin.mjs"
  },
  "devDependencies": {
    "esbuild": "^0.15.15",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "puppeteer": "^19.2.2"
  }
}
