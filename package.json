{
  "private": true,
  "name": "clickhouse-bot",
  "version": "0.0.0",
  "engines": {
    "node": "^20"
  },
  "packageManager": "yarn@3.3.0",
  "type": "module",
  "main": "dist/index.js",
  "bin": "dist/bin.js",
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
