{
  "private": true,
  "name": "clickhouse-bot",
  "packageManager": "yarn@3.3.0",
  "type": "module",
  "main": "src/index.mjs",
  "bin": "./bin.cjs",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "prettier -c .",
